.video {
  position: relative;
  width: 230px;
  height: 192px
  display: flex;
  align-items center
  justify-content center


  &__thumb-container {
    position: absolute;
    z-index 2
    top: 0
    left 0
    width: 100%
    height: 100%
  }

  &__thumb {
    width: 100%
  }

  &__video-container {
    position: absolute;
    z-index 1
    top 0
    left 0
    width: 100%
    height: 100%
  }
}

